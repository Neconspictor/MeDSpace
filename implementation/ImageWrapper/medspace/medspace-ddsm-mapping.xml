<?xml version="1.0"?>
<ddsm:Root xmlns:ddsm="http://www.medspace.com/images/ddsm">

	<!-->RDF namespaces
		@attribute prefix:		The prefix to use in the rdf triples
		@attribute namespace:	The namespace represented by the prefix
	<-->
	<ddsm:Namespace prefix="root" namespace="http://www.medspace.com/images/ddsm/"/>
	<ddsm:Namespace prefix="owl" namespace="http://www.w3.org/2002/07/owl#"/>
	<ddsm:Namespace prefix="rdf" namespace="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
	<ddsm:Namespace prefix="dc" namespace="http://purl.org/dc/elements/1.1/"/>
	<ddsm:Namespace prefix="xsd" namespace="http://www.w3.org/2001/XMLSchema#"/>

	<ddsm:Namespace prefix="ddsm_container" namespace="http://www.medspace.com/images/ddsm/ddsm_container#"/>
    <ddsm:Namespace prefix="image" namespace="http://www.medspace.com/images/ddsm/ddsm_container/image#"/>
    <ddsm:Namespace prefix="overlay" namespace="http://www.medspace.com/images/ddsm/ddsm_container/overlay#"/>
    <ddsm:Namespace prefix="abnormality" namespace="http://www.medspace.com/images/ddsm/ddsm_container/overlay/abnormality#"/>
    <ddsm:Namespace prefix="lesion_type" namespace="http://www.medspace.com/images/ddsm/ddsm_container/overlay/abnormality/lesion_type#"/>


    <!-->IcsFile
		each exported ddsm container will get a unique URI:
		type#ics-filename

		 -> where type is the content of the DDSMContainer attribute 'type'
		 -> where ics-filename is the relative filename path to the *.ics file, representing
		 the created DDSMContainer object
	<-->

    <ddsm:IcsFile rdfType="root:ddsm_container" classId="IcsFile">
        <ddsm:dateOfStudy propertyType="ddsm_container:dateOfStudy" dataType="xsd:date"/>
        <ddsm:patientAge propertyType="ddsm_container:patientAge" dataType="xsd:integer"/>
        <ddsm:density propertyType="ddsm_container:density" dataType="xsd:integer"/>
        <ddsm:dateDigitized propertyType="ddsm_container:dateDigitized" dataType="xsd:date"/>
        <ddsm:digitizer propertyType="ddsm_container:digitizer" dataType="xsd:string" lang="en"/>
        <ddsm:left_cc propertyType="ddsm_container:left_cc"/>
        <ddsm:left_mlo propertyType="ddsm_container:left_mlo"/>
        <ddsm:right_cc propertyType="ddsm_container:right_cc"/>
        <ddsm:right_mlo propertyType="ddsm_container:right_mlo"/>
    </ddsm:IcsFile>

    <ddsm:Image rdfType="ddsm_container:image" classId="Image">
        <ddsm:source propertyType="image:source" dataType="xsd:string"/>
        <ddsm:overlay propertyType="image:overlay"/>
    </ddsm:Image>

    <ddsm:Overlay rdfType="ddsm_container:overlay" classId="Overlay">
        <ddsm:totalAbnormalities propertyType="overlay:totalAbnormalities" dataType="xsd:integer"/>
        <ddsm:abnormality propertyType="overlay:abnormality"/>
    </ddsm:Overlay>
    
    <ddsm:Abnormality rdfType="overlay:abnormality" classId="Abnormality">
        <ddsm:abnormality propertyType="abnormality:abnormality" dataType="xsd:integer"/>
        <ddsm:calcification propertyType="abnormality:calcification"/>
        <ddsm:mass propertyType="abnormality:mass"/>
        <ddsm:assessment propertyType="abnormality:assessment" dataType="xsd:integer"/>
        <ddsm:subtlety propertyType="abnormality:subtlety" dataType="xsd:integer"/>
        <ddsm:pathology propertyType="abnormality:pathology" dataType="xsd:string"/>
        <ddsm:totalOutlines propertyType="abnormality:totalOutlines" dataType="xsd:integer"/>
    </ddsm:Abnormality>

    <ddsm:Calcification rdfType="lesion_type:calcification" classId="Calcification">
        <ddsm:type propertyType="lesion_type:type" dataType="xsd:string"/>
        <ddsm:distribution propertyType="lesion_type:distribution" dataType="xsd:string"/>
    </ddsm:Calcification>

    <ddsm:Mass rdfType="lesion_type:mass" classId="Mass">
        <ddsm:shape propertyType="lesion_type:shape" dataType="xsd:string"/>
        <ddsm:margins propertyType="lesion_type:margins" dataType="xsd:string"/>
    </ddsm:Mass>

</ddsm:Root>