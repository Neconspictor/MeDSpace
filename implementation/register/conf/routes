# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.RegisterController.index

GET     /add                        controllers.RegisterController.add

GET     /test1                        controllers.RegisterController.test1

GET     /test2                        controllers.RegisterController.test2

GET     /block1                        controllers.RegisterController.blockTest1
GET     /block2                        controllers.RegisterController.blockTest2
GET     /block3                        controllers.RegisterController.blockTest3

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
