# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                            controllers.RegisterController.index

+nocsrf
POST     /add                        controllers.RegisterController.add

GET /get-datasources                  controllers.RegisterController.getDatasources

+nocsrf
POST     /remove                     controllers.RegisterController.remove

+nocsrf
POST     /no-response                 controllers.RegisterController.noResponse

GET     /javascript-routes            controllers.RegisterController.javascriptRoutes


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                controllers.Assets.versioned(path="/public", file: Asset)

# QueryExecutor stuff
GET     /query-executor-test           controllers.QueryExecutorController.queryExecutorTest(query ?="")

# DataCollector stuff
GET     /data-collector/create-query-result     controllers.DataCollectorController.createQueryResult()

+nocsrf
POST     /data-collector/add-partial-query-result     controllers.DataCollectorController.addPartialQueryResultInputStream(resultID, rdfFormat, baseURI)

GET     /data-collector/delete-query-result          controllers.DataCollectorController.deleteQueryResult(resultID)

GET     /data-collector/query-result          controllers.DataCollectorController.queryResult(resultID, rdfFormat)
