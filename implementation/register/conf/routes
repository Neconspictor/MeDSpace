# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                            controllers.RegisterController.index

+nocsrf
POST     /add                        controllers.RegisterController.add

GET /getDatasources                  controllers.RegisterController.getDatasources

+nocsrf
POST     /remove                     controllers.RegisterController.remove

+nocsrf
POST     /noResponse                 controllers.RegisterController.noResponse

GET     /javascriptRoutes            controllers.RegisterController.javascriptRoutes


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file                controllers.Assets.versioned(path="/public", file: Asset)

# QueryExecutor stuff
GET     /queryExecutorTest           controllers.QueryExecutorController.queryExecutorTest
