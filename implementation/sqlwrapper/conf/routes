# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.SQLWrapperController.index

GET     /testConnection             controllers.SQLWrapperController.testDatasourceConnection

GET /fileTest                 controllers.SQLWrapperController.fileTest

GET /search                         controllers.SQLWrapperController.search(attach:Boolean ?= true)

GET /test                       controllers.SQLWrapperController.test

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
